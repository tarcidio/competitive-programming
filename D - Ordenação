#include <iostream>
 
using namespace std;
 
int main (){
    
    int a, b, c;
    cin >> a >> b >> c;
    
    if(a >= b && b >= c)
        cout << a << '\n' << b << '\n' << c;
    else if(a >= c && c >= b)
        cout << a << '\n' << c << '\n' << b;
    else if(b >= a && a >= c)
        cout << b << '\n' << a << '\n' << c;
    else if(b >= c && c >= a)
        cout << b << '\n' << c << '\n' << a;
    else if(c >= a && a >= b)
        cout << c << '\n' << a << '\n' << b;
    else if(c >= b && b >= a)
        cout << c << '\n' << b << '\n' << a;
    
    return 0;
}
